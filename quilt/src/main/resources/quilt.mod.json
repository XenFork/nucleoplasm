{
  "schema_version": 1,
  "mixin": [
    "${common_mixins}"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "nucleoplasm",
    "version": "${version}",
    "name": "Nucleoplasm Mod",
    "description": "",
    "authors": [
      "${authors}"
    ],
    "contact": {
      "issues": "${issues_url}"
    },
    "license": "${license_type}",
    "icon": "icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "io.github.xenfork.nucleoplasm.quilt.NucleoplasmQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.1"
      },
      {
        "id": "architectury",
        "version": ">=9.1.12"
      }
    ]
  }
}